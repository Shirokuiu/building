.house__preview
  position absolute
  top 20px
  left 0
  z-index 20
  
  opacity 1
  
  transition opacity 0.5s ease
  &--red
    .house__preview-items
      &--green
        background-color #e6202f
        
        transition 0.2s ease
    .house__preview-number,
    .house__preview-text
      color #e7202f
      &--white
        color #ffffff
  &--hidden
    opacity 0
  
.house__popup
  center-absolute()
  
  z-index 30
  
  display flex
  justify-content center
  align-items center
  
  background-color rgba(000, 000, 000, 0.6);
  &--rooms6
    .house__popup-img
      background-image url('%=static=%content/popup-rooms6-img.png')
  
.house__popup-content
