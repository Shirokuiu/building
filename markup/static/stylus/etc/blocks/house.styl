.house__preview
  position absolute
  top 20px
  left 0
  z-index 20
  
  opacity 1
  
  transition opacity 0.5s ease
  &--red
    .house__preview-items
      &--green
        background-color #e6202f
        
        transition 0.2s ease
    .house__preview-number,
    .house__preview-text
      color #e7202f
      &--white
        color #ffffff
  &--hidden
